{
  "name": "core",
  "version": "0.0.1",
  "description": "",
  "author": "Oleh Strokan",
  "private": true,
  "main": "src/main.ts",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn install",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=local node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=dev jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "NODE_ENV=prod yarn typeorm migration:run -- -d ./src/shared/database/database.datasource.ts",
    "migration:test:run": "NODE_ENV=dev NODE_ENV=dev yarn typeorm migration:run -- -d ./src/shared/testing/database/database.datasource.ts",
    "migration:test:generate": "NODE_ENV=dev yarn typeorm -- -d ./src/shared/testing/database/database.datasource.ts migration:generate ./src/shared/testing/database/migrations/$npm_config_name",
    "migration:generate": "NODE_ENV=prod yarn typeorm -- -d ./src/shared/database/database.datasource.ts migration:generate ./src/shared/database/migrations/$npm_config_name",
    "migration:create": "NODE_ENV=dev yarn typeorm -- migration:create ./src/shared/database/migrations/$npm_config_name",
    "migration:revert": "NODE_ENV=dev yarn typeorm -- -d ./src/shared/database/database.datasource.ts migration:revert",
    "migration:show": "NODE_ENV=dev yarn typeorm migration:show -- -d ./src/shared/database/database.datasource.ts"
  },
  "dependencies": {
    "@langchain/community": "^0.0.40",
    "@langchain/core": "^0.3.58",
    "@langchain/openai": "^0.5.13",
    "@nestjs/common": "^11.0.4",
    "@nestjs/config": " ^4.0.0",
    "@nestjs/core": "^11.0.4",
    "@nestjs/microservices": "^11.0.4",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "langchain": "^0.3.28",
    "openai": "^5.3.0",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.20",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-extended": "^4.0.2",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
