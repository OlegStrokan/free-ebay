{
  "name": "core",
  "version": "0.0.1",
  "description": "",
  "author": "Oleh Strokan",
  "private": true,
  "main": "src/main.ts",
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "install:clean": "rm -rf node_modules && rm yarn.lock && yarn install",
    "start": "nest start",
    "start:dev": "NODE_ENV=dev nest start --watch",
    "start:debug": "nest dev --debug --watch",
    "start:prod": "NODE_ENV=dev node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "//": "update .test.env and use for testing script",
    "test": "NODE_ENV=test jest --runInBand",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "NODE_ENV=dev yarn typeorm migration:run -d ./src/shared/database/database.datasource.ts",
    "migration:test:run": "NODE_ENV=test yarn typeorm migration:run  -d ./src/shared/database/database.datasource.ts",
    "migration:test:generate": "NODE_ENV=test yarn typeorm -d /src/shared/database/database.datasource.ts migration:generate ./src/shared/database/migrations/$npm_config_name",
    "migration:generate": "NODE_ENV=dev yarn typeorm -d ./src/shared/database/database.datasource.ts migration:generate ./src/shared/database/migrations/$1",
    "migration:create": "NODE_ENV=dev yarn typeorm migration:create ./src/shared/database/migrations/$npm_config_name",
    "migration:revert": "NODE_ENV=dev yarn typeorm -d ./src/shared/database/database.datasource.ts migration:revert",
    "migration:show": "NODE_ENV=dev yarn typeorm migration:show -d ./src/shared/database/database.datasource.ts"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.1.1",
    "@grpc/grpc-js": "^1.13.4",
    "@langchain/community": "^0.3.53",
    "@langchain/core": "^0.3.72",
    "@langchain/openai": "^0.6.9",
    "@langchain/tavily": "^0.1.5",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": " ^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/elasticsearch": "^11.1.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "elasticsearch": "^16.7.3",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "langchain": "^0.3.31",
    "openai": "^5.15.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.26",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/testing": "^11.1.6",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/glob": "^9.0.0",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "jest-extended": "^6.0.0",
    "prettier": "^3.6.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "preset": "ts-jest",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    },
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 10000,
    "forceExit": true,
    "detectOpenHandles": true
  }
}
